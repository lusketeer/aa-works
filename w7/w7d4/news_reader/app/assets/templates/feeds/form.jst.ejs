<form>
  <div class="form-group">
    <label>
      Title
      <div class="form-control">
        <input type="text" name="feed[title]" value="<%= feed.escape('title')%>">
      </div>
    </label>
  </div>
  <div class="form-group">
    <label>
      URL
      <div class="form-control">
        <input type="text" name="feed[url]" value="<%= feed.escape('url')%>">
      </div>
    </label>
  </div>
  <div class="form-group">
    <div class="form-control">
      <button class="submit">Create Feed</button>
    </div>
  </div>
</form>
